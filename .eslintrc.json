{
  "env": {
    "browser": true,
    "es2021": true
  },
  "parser": "@babel/eslint-parser",
  "extends": [
    "standard"
  ],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "indent": ["error", 2],
    "semi": ["error", "always"],
    "no-var": "error",
    "no-mixed-spaces-and-tabs": 0,
    "accessor-pairs": 0,
    "no-tabs": 0
  }
}
